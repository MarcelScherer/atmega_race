                                 .equ STA_START     = 0
                                 .equ STA_ROUND_1   = 1
                                 .equ DELAY_TIME    = 2
                                 
                                 .def temp1 = r16
                                 .def temp2 = r17
                                 .def temp3 = r18
                                 .def Flag = r19
                                 .def SubCount    = r21
                                 .def mil_sec     = r22
                                 .def second      = r23
                                 .def minute      = r24
                                 .def timer_state = r25
                                 #define delay_timer XL ;  use register r26
                                 
                                 .org 0x0000
000000 c07d                      	rjmp main							; reset hanlder -> jump to main
                                 .org OC1Aaddr
000006 c0ac                      	rjmp timer1_compare					; Timer Compare Handler -> jump to timer1_compare
                                 
                                 .include "lcd-routines.asm"				; include lcd library
                                 
                                 ;;                 LCD-Routinen                ;;
                                 ;;                 ============                ;;
                                 ;;              (c)andreas-s@web.de            ;;
                                 ;;                                             ;;
                                 ;; 4bit-Interface                              ;;
                                 ;; DB4-DB7:       PD0-PD3                      ;;
                                 ;; RS:            PD4                          ;;
                                 ;; E:             PD5                          ;;
                                 ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                                  
                                 .equ LCD_PORT = PORTD
                                 .equ LCD_DDR  = DDRD
                                 .equ PIN_RS   = 4
                                 .equ PIN_E    = 5
                                 
                                 .ifndef XTAL
                                 .equ XTAL = 4000000
                                 .endif
                                 
                                 ; Eine Zahl aus dem Register temp1 ausgeben
                                 lcd_number:
000007 931f                      	push temp2							; register sichern,
                                 										; wird fr Zwsichenergebnisse gebraucht
000008 e310                      	ldi temp2, '0'
                                 lcd_number_10:
000009 500a                      	subi temp1, 10						; abzhlen wieviele Zehner in
00000a f010                      	brcs lcd_number_1					; der Zahl enthalten sind
00000b 9513                      	inc temp2
00000c cffc                      	rjmp lcd_number_10
                                 lcd_number_1:
00000d 930f                      	push temp1
00000e 2f01                      	mov temp1, temp2
00000f d007                      	rcall lcd_data						; die Zehnerstelle ausgeben
000010 910f                      	pop temp1
000011 5f06                      	subi temp1, -10						; 10 wieder dazuzhlen, da die
                                 										; vorhergehende Schleife 10 zuviel
                                 										; abgezogen hat
                                 										; das Subtrahieren von -10
                                 										; = Addition von +10 ist ein Trick
                                 										; da kein addi Befehl existiert
000012 e310                      	ldi temp2, '0'						; die brig gebliebenen Einer
000013 0f01                      	add temp1, temp2					; noch ausgeben
000014 d002                      	rcall lcd_data
000015 911f                      	pop temp2							; Register wieder herstellen
000016 9508                      	ret
                                 
                                  ;sendet ein Datenbyte an das LCD
                                 lcd_data:
000017 931f                                 push  temp2
000018 932f                                 push  temp3
000019 2f10                                 mov   temp2, temp1            ; "Sicherungskopie" fr
                                                                          ; die bertragung des 2.Nibbles
00001a 9502                                 swap  temp1                   ; Vertauschen
00001b 700f                                 andi  temp1, 0b00001111       ; oberes Nibble auf Null setzen
00001c 6100                                 sbr   temp1, 1<<PIN_RS        ; entspricht 0b00010000
00001d b322                                 in    temp3, LCD_PORT
00001e 7820                                 andi  temp3, 0x80
00001f 2b02                                 or    temp1, temp3
000020 bb02                                 out   LCD_PORT, temp1         ; ausgeben
000021 d01b                                 rcall lcd_enable              ; Enable-Routine aufrufen
                                                                          ; 2. Nibble, kein swap da es schon
                                                                          ; an der richtigen stelle ist
000022 701f                                 andi  temp2, 0b00001111       ; obere Hlfte auf Null setzen 
000023 6110                                 sbr   temp2, 1<<PIN_RS        ; entspricht 0b00010000
000024 2b12                                 or    temp2, temp3
000025 bb12                                 out   LCD_PORT, temp2         ; ausgeben
000026 d016                                 rcall lcd_enable              ; Enable-Routine aufrufen
000027 d01b                                 rcall delay50us               ; Delay-Routine aufrufen
                                 
000028 912f                                 pop   temp3
000029 911f                                 pop   temp2
00002a 9508                                 ret                           ; zurck zum Hauptprogramm
                                  
                                  ; sendet einen Befehl an das LCD
                                 lcd_command:                            ; wie lcd_data, nur ohne RS zu setzen
00002b 931f                                 push  temp2
00002c 932f                                 push  temp3
                                 
00002d 2f10                                 mov   temp2, temp1
00002e 9502                                 swap  temp1
00002f 700f                                 andi  temp1, 0b00001111
000030 b322                                 in    temp3, LCD_PORT
000031 7820                                 andi  temp3, 0x80
000032 2b02                                 or    temp1, temp3
000033 bb02                                 out   LCD_PORT, temp1
000034 d008                                 rcall lcd_enable
000035 701f                                 andi  temp2, 0b00001111
000036 2b12                                 or    temp2, temp3
000037 bb12                                 out   LCD_PORT, temp2
000038 d004                                 rcall lcd_enable
000039 d009                                 rcall delay50us
                                  
00003a 912f                                 pop   temp3
00003b 911f                                 pop   temp2
00003c 9508                                 ret
                                  
                                  ; erzeugt den Enable-Puls
                                 lcd_enable:
00003d 9a95                                 sbi LCD_PORT, PIN_E          ; Enable high
00003e 0000                                 nop                          ; 3 Taktzyklen warten
00003f 0000                                 nop
000040 0000                                 nop
000041 9895                                 cbi LCD_PORT, PIN_E          ; Enable wieder low
000042 9508                                 ret                          ; Und wieder zurck                     
                                  
                                  ; Pause nach jeder bertragung
                                 delay50us:                              ; 50us Pause
000043 e402                                 ldi  temp1, ( XTAL * 50 / 3 ) / 1000000
                                 delay50us_:
000044 950a                                 dec  temp1
000045 f7f1                                 brne delay50us_
000046 9508                                 ret                          ; wieder zurck
                                  
                                  ; Lngere Pause fr manche Befehle
                                 delay5ms:                               ; 5ms Pause
000047 e200                                 ldi  temp1, ( XTAL * 5 / 607 ) / 1000
000048 ec19                      WGLOOP0:   ldi  temp2, $C9
000049 951a                      WGLOOP1:   dec  temp2
00004a f7f1                                 brne WGLOOP1
00004b 950a                                 dec  temp1
00004c f7d9                                 brne WGLOOP0
00004d 9508                                 ret                          ; wieder zurck
                                  
                                  ; Initialisierung: muss ganz am Anfang des Programms aufgerufen werden
                                 lcd_init:
00004e 930f                                 push  temp1
00004f b301                                 in    temp1, LCD_DDR
000050 630f                                 ori   temp1, (1<<PIN_E) | (1<<PIN_RS) | 0x0F
000051 bb01                                 out   LCD_DDR, temp1
                                 
000052 e026                                 ldi   temp3,6
                                 powerupwait:
000053 dff3                                 rcall delay5ms
000054 952a                                 dec   temp3
000055 f7e9                                 brne  powerupwait
000056 e003                                 ldi   temp1,    0b00000011   ; muss 3mal hintereinander gesendet
000057 bb02                                 out   LCD_PORT, temp1        ; werden zur Initialisierung
000058 dfe4                                 rcall lcd_enable             ; 1
000059 dfed                                 rcall delay5ms
00005a dfe2                                 rcall lcd_enable             ; 2
00005b dfeb                                 rcall delay5ms
00005c dfe0                                 rcall lcd_enable             ; und 3!
00005d dfe9                                 rcall delay5ms
00005e e002                                 ldi   temp1,    0b00000010   ; 4bit-Modus einstellen
00005f bb02                                 out   LCD_PORT, temp1
000060 dfdc                                 rcall lcd_enable
000061 dfe5                                 rcall delay5ms
000062 e208                                 ldi   temp1,    0b00101000   ; 4 Bot, 2 Zeilen
000063 dfc7                                 rcall lcd_command
000064 e00c                                 ldi   temp1,    0b00001100   ; Display on, Cursor off
000065 dfc5                                 rcall lcd_command
000066 e004                                 ldi   temp1,    0b00000100   ; endlich fertig
000067 dfc3                                 rcall lcd_command
                                 
000068 910f                                 pop   temp1
000069 9508                                 ret
                                  
                                  ; Sendet den Befehl zur Lschung des Displays
                                 lcd_clear:
00006a 930f                                 push  temp1
00006b e001                                 ldi   temp1,    0b00000001   ; Display lschen
00006c dfbe                                 rcall lcd_command
00006d dfd9                                 rcall delay5ms
00006e 910f                                 pop   temp1
00006f 9508                                 ret
                                 
                                  ; Einen konstanten Text aus dem Flash Speicher
                                  ; ausgeben. Der Text wird mit einer 0 beendet
                                 lcd_flash_string:
000070 930f                                 push  temp1
                                 
                                 lcd_flash_string_1:
000071 9105                                 lpm   temp1, Z+
000072 3000                                 cpi   temp1, 0
000073 f011                                 breq  lcd_flash_string_2
000074 dfa2                                 rcall  lcd_data
000075 cffb                                 rjmp  lcd_flash_string_1
                                 
                                 lcd_flash_string_2:
000076 910f                                 pop   temp1
000077 9508                                 ret
                                 
                                  ; Cursor Home
                                 lcd_home:
000078 930f                                 push  temp1
000079 e002                                 ldi   temp1,    0b00000010   ; Cursor Home
00007a dfb0                                 rcall lcd_command
00007b dfcb                                 rcall delay5ms
00007c 910f                                 pop   temp1
00007d 9508                                 ret
                                 
                                 main:
                                 	; initialise stack pointer
00007e e50f                      	ldi temp1, LOW(RAMEND)				
00007f bf0d                      	out SPL, temp1
000080 e004                      	ldi temp1, HIGH(RAMEND)
000081 bf0e                      	out SPH, temp1
                                 
                                 	; init and clear display
000082 dfcb                      	rcall lcd_init
000083 dfe6                      	rcall lcd_clear
                                 
                                 	; init in- and output pints
000084 e002                      	ldi temp1,0b00000010
000085 bb07                      	out DDRB,temp1						; config PB1 as output and all other PBx -Pins as input
                                 
                                 	; init timer for 100 interrupts per second
000086 e90c                      	ldi temp1, high( 40000 - 1 )
000087 bd0b                      	out OCR1AH, temp1
000088 e30f                      	ldi temp1, low( 40000 - 1 )
000089 bd0a                      	out OCR1AL, temp1
00008a e009                      	ldi temp1, ( 1 << WGM12 ) | ( 1 << CS10 )
00008b bd0e                      	out TCCR1B, temp1
00008c e100                      	ldi temp1, 1 << OCIE1A				; OCIE1A: Interrupt bei Timer Compare
00008d bf09                      	out TIMSK, temp1
                                 
                                 	; clear all variable
00008e 2788                      	clr minute 
00008f 2777                      	clr second
000090 2766                      	clr mil_sec
000091 2755                      	clr SubCount
000092 e031                      	ldi Flag,1							; update display
000093 e090                      	ldi timer_state,STA_START
000094 27aa                      	clr delay_timer;
000095 9478                      	sei
                                 
                                 loop:
000096 e002                      	ldi temp1, 0b00000010
000097 bb08                      	out PORTB, temp1
000098 9bb0                      	sbis PINB, 0						; scip jmp loop_1 if bit 1 is HIGH
000099 c006                      	rjmp loop_1							; jump to loop_1
